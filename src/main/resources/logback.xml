<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d %-5p [%.30t] %-30.30c{1} %m%n</pattern>
    </encoder>
  </appender>
  <root level="info">
    <appender-ref ref="consoleAppender"/>
  </root>
</configuration>
